
{
    "manifest_version": 3,
    "name": "Note Taker",
    "version": "1.1",
    "description": "A simple note-taking Chrome extension.",
    "action": {
      "default_popup": "index.html",
      "default_icon": "images/icon.png"
    },
    "permissions": ["storage"],
    "background": {
      "service_worker": "background.js"
    }
  }
  